// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_DB_URL")

}

model User {
  id            String    @id @default(cuid())
  name          String?
  email         String?  
  provider      String? 
  password      String? 
  image         String?
}
